<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="pswManage_api" extends="wirich">
		<!-- 修改交易密码430103 -->
		<action name="changePsw" class="PswManageAction" method="changePsw">
		</action>

		<!-- 查询用户信息是否存在 -->
		<action name="queryForCustnoInfo" class="PswManageAction"
			method="queryForCustnoInfo">
		</action>

		<!-- 找回交易密码490024 -->
		<action name="getBackPsw" class="PswManageAction" method="getBackPsw">
		</action>

		<!-- 查询单客户持有基金分红方式 -->
		<action name="querysbonus" class="PswManageAction"
			method="querySingleCustomerHoldingFundDividends">
		</action>
		
		<!-- 分红方式变更 -->
		<action name="changesbonus" class="PswManageAction"
			method="changeTheWayOfDividends">
		</action>
	</package>
</struts>



