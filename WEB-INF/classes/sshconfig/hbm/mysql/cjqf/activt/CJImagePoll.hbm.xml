<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wirich.bean.cjqf.activit.ImagePollBean" table="cj_image_poll">
		<!-- 主键Id -->
		<id name="id" type="java.lang.String">
			<column name="id" />
		</id>
		<!-- 轮播序号  -->
		<property name="imgSort" type="int">
			<column name="img_sort" />
		</property>
		<!-- 活动主键id  -->
		<property name="activtId" type="java.lang.String">
			<column name="activit_id" />
		</property>
		<!--  图片更新时间戳 -->
		<property name="imgPath" type="java.lang.String">
			<column name="img_path" />
		</property>
		<!-- 创建人   -->
		<property name="createPerson" type="java.lang.String">
			<column name="create_person" />
		</property>
		<!--  创建时间  -->
		<property name="createTime" type="java.util.Date">
			<column name="create_time" />
		</property>
		<!--  更新人 -->
		<property name="updatePerson" type="java.lang.String">
			<column name="update_person" />
		</property>
		<!--  更新时间  -->
		<property name="updateTime" type="java.util.Date">
			<column name="update_time" />
		</property>
	</class>
</hibernate-mapping>
