<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wirich.bean.cjqfweb.group.CJQFWebFundGroupFundBean" table="cjqfweb_fund_group_fund">
		<!-- 主键Id -->
		<id name="id" type="int">
			<column name="id" />
			<generator class="increment" />
		</id>
		<!--  基金组合代码(唯一) -->
		<property name="groupId" type="java.lang.String">
			<column name="fd_group_code" />
		</property>
		<!--  基金组合名称  -->
		<property name="fdGroupName" type="java.lang.String">
			<column name="fd_group_name" />
		</property>
		<!--基金代码-->
		<property name="fundId" type="java.lang.String">
			<column name="fund_id" />
		</property>
		<!--基金名称-->
		<property name="fundName" type="java.lang.String">
			<column name="fund_name" />
		</property>
		<!--基金类型-->
		<property name="fundType" type="java.lang.String">
			<column name="fund_type" />
		</property>
		<!--基金占比 -->
		<property name="fdProportional" type="java.math.BigDecimal">
			<column name="fd_proportional" />
		</property>
		<!--基金说明 -->
		<property name="fdInfo" type="java.lang.String">
			<column name="fd_info" />
		</property>
		<!--组成基金点评-->
		<property name="fdGroupInfo" type="java.lang.String">
			<column name="fd_group_info" />
		</property>
		<!--组成基金标识-->
		<property name="fdGroupSign" type="java.lang.String">
			<column name="fd_group_sign" />
		</property>
		<!--  创建人	 -->
		<property name="createPerson" type="java.lang.String">
			<column name="create_person" />
		</property>
		<!-- 创建时间 -->
		<property name="createTime" type="java.lang.String">
			<column name="create_time" />
		</property>
		<!-- 更新人 -->
		<property name="updatePerson" type="java.lang.String">
			<column name="update_person" />
		</property>
		<!-- 更新时间 -->
		<property name="updateTime" type="java.lang.String">
			<column name="update_time" />
		</property>
	</class>
</hibernate-mapping>