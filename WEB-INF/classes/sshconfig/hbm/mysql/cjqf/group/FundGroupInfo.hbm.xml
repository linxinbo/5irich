<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wirich.bean.cjqf.group.FundGroupInfoBean" table="cj_fund_group_info">
		<!-- 主键Id -->
		<id name="id" type="java.lang.String">
			<column name="id" />
		</id>
		<!--  基金组合ID	 -->
		<property name="groupId" type="java.lang.String">
			<column name="group_id" />
		</property>
		<!--  费率说明	 -->
		<property name="fdGroupFee" type="java.lang.String">
			<column name="fd_group_fee" />
		</property>
		<!--  最近交易日	  -->
		<property name="lastTradeDate" type="java.lang.String">
			<column name="last_trade_date" />
		</property>
		<!--  购买方式  -->
		<property name="fdBuyDesc" type="java.lang.String">
			<column name="fd_buy_desc" />
		</property>
		<!--  收益率说明	 -->
		<property name="fdProfitDesc" type="java.lang.String">
			<column name="fd_profit_desc" />
		</property>
		<!--  风险提示 -->
		<property name="fdRiskDesc" type="java.lang.String">
			<column name="fd_risk_desc" />
		</property>
		<!--  交易说明 -->
		<property name="fdDualDesc" type="java.lang.String">
			<column name="fd_dual_desc" />
		</property>
		<!--  投资说明 -->
		<property name="fdInvestDesc" type="java.lang.String">
			<column name="fd_invest_desc" />
		</property>
		<!--  其他说明 -->
		<property name="fdOther" type="java.lang.String">
			<column name="fd_other" />
		</property>
		<!--  创建人	 -->
		<property name="createPerson" type="java.lang.String">
			<column name="create_person" />
		</property>
		<!-- 创建时间 -->
		<property name="createTime" type="java.util.Date">
			<column name="create_time" />
		</property>
		<!--  更新人  -->
		<property name="updatePerson" type="java.lang.String">
			<column name="update_person" />
		</property>
		<!--  更新时间  -->
		<property name="updateTime" type="java.util.Date">
			<column name="update_time" />
		</property>
	</class>
</hibernate-mapping>
