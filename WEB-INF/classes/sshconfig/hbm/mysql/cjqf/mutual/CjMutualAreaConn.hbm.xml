<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wirich.bean.cjqf.mutual.CjMutualAreaConnBean" table="cj_mutual_fund_area_conn">
		<!-- 主键Id -->
		<id name="id" type="java.lang.String">
			<column name="id" />
		</id>
		<!-- 基金主表ID -->
		<property name="fundId" type="java.lang.String">
			<column name="fund_id" />
		</property>
		<!-- 区域主键ID  -->
		<property name="areaId" type="java.lang.String">
			<column name="area_id" />
		</property>
		<!--  推荐级别-->
		<property name="recommendLevel" type="java.lang.Integer">
			<column name="recommend_level" />
		</property>
		<!--  序号  -->
		<property name="sort" type="java.lang.Integer">
			<column name="sort" />
		</property>
		<!--  创建人	 -->
		<property name="createPerson" type="java.lang.String">
			<column name="create_person" />
		</property>
		<!-- 创建时间 -->
		<property name="createTime" type="java.util.Date">
			<column name="create_time" />
		</property>
		<!-- 更新人 -->
		<property name="updatePerson" type="java.lang.String">
			<column name="update_person" />
		</property>
		<!-- 更新时间 -->
		<property name="updateTime" type="java.util.Date">
			<column name="update_time" />
		</property>
	</class>
</hibernate-mapping>
