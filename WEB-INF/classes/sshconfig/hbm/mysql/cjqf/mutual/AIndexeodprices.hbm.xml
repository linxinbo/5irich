<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wirich.bean.cjqf.mutual.AIndexeodpricesBean"
		table="aindexeodprices">

		<!--   交易日期 -->
		<id name="tradeDt" type="java.lang.String">
			<column name="TRADE_DT" />
		</id>
		
		<!--  收盘价(点) -->
		<property name="sDqClose" type="java.math.BigDecimal">
			<column name="S_DQ_CLOSE" />
		</property>
		
	</class>
</hibernate-mapping>
