<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wirich.bean.cjqf.mutual.CjMutualFundInfoBean" table="cj_mutual_fund_info">
		<!-- 主键Id -->
		<id name="id" type="java.lang.String">
			<column name="id" />
		</id>
		<!--  基金主表ID -->
		<property name="fdId" type="java.lang.String">
			<column name="fd_id" />
		</property>
		<!--  已经购买人数  -->
		<property name="buyCount" type="java.lang.Integer">
			<column name="buy_count" />
		</property>
		<!--   申购费率(%) -->
		<property name="rate" type="java.math.BigDecimal">
			<column name="rate" />
		</property>
		<!-- 认购费率(%) -->
		<property name="firstRate" type="java.math.BigDecimal">
			<column name="first_rate" />
		</property>
		<!-- 折扣费率(%) -->
		<property name="discountRate" type="java.math.BigDecimal">
			<column name="discount_rate" />
		</property>
		<!--  基金状态 -->
		<property name="fdState" type="java.lang.Integer">
			<column name="fd_state" />
		</property>
		<!--  操作类型  -->
		<property name="operateType" type="java.lang.Integer">
			<column name="operate_type" />
		</property>
		<!--  创建人	 -->
		<property name="createPerson" type="java.lang.String">
			<column name="create_person" />
		</property>
		<!-- 创建时间 -->
		<property name="createTime" type="java.util.Date">
			<column name="create_time" />
		</property>
		<!-- 更新人 -->
		<property name="updatePerson" type="java.lang.String">
			<column name="update_person" />
		</property>
		<!-- 更新时间 -->
		<property name="updateTime" type="java.util.Date">
			<column name="update_time" />
		</property>
	</class>
</hibernate-mapping>
