<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wirich.bean.cjqf.mutual.CjFutualFundMainBean" table="cj_mutual_fund_main">
		<!-- 主键Id -->
		<id name="id" type="java.lang.String">
			<column name="id" />
		</id>
		<!--  基金代码 -->
		<property name="fundcCde" type="java.lang.String">
			<column name="fund_code" />
		</property>
		<!--  Wind代码  -->
		<property name="windCde" type="java.lang.String">
			<column name="wind_code" />
		</property>
		<!--  基金简称 -->
		<property name="name" type="java.lang.String">
			<column name="name" />
		</property>
		<!--  基金全称  -->
		<property name="fullName" type="java.lang.String">
			<column name="full_name" />
		</property>
		<!--  成立日期  -->
		<property name="setupDate" type="java.lang.String">
			<column name="setup_date" />
		</property>
		<!--  基金公司 -->
		<property name="companyId" type="java.lang.String">
			<column name="company_id" />
		</property>
		<!--  基金类型  -->
		<property name="fundType" type="java.lang.Integer">
			<column name="fund_type" />
		</property>
		<!--  托管银行  -->
		<property name="custodianBank" type="java.lang.Integer">
			<column name="custodian_bank" />
		</property>
		<!-- 基金状态  -->
		<property name="fdState" type="java.lang.Integer">
			<column name="fd_state" />
		</property>
		<!-- 操作类型  -->
		<property name="operateType" type="java.lang.Integer">
			<column name="operate_type" />
		</property>
		
		<!--  投资起点 -->
		<property name="minBuyAmount" type="java.math.BigDecimal">
			<column name="min_buy_amount" />
		</property>
		<!--  优选理由-->
		<property name="priorityDesc" type="java.lang.String">
			<column name="priority_desc" />
		</property>
		<!--  创建人	 -->
		<property name="createPerson" type="java.lang.String">
			<column name="create_person" />
		</property>
		<!-- 创建时间 -->
		<property name="createTime" type="java.util.Date">
			<column name="create_time" />
		</property>
		<!-- 更新人 -->
		<property name="updatePerson" type="java.lang.String">
			<column name="update_person" />
		</property>
		<!-- 更新时间 -->
		<property name="updateTime" type="java.util.Date">
			<column name="update_time" />
		</property>
	</class>
</hibernate-mapping>
