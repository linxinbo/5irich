<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.wirich.bean.accountbook.cjqfweb.CjqfwebMutualDividendMockAccountBookBean" table="cjqfweb_mutual_dividend_mockaccountbook" >
        <id name="id" type="int">
            <column name="ID"/>
        </id>
        <property name="userId" type="string">
            <column name="userid" />
        </property>
        <property name="fundName" type="string">
            <column name="fundname" />
        </property>
        <property name="fundCode" type="string">
            <column name="fundcode" />
        </property>
        <property name="dividendDate" type="string">
            <column name="dividenddate" />
        </property>
        <property name="dividendShare" type="java.math.BigDecimal">
            <column name="dividendshare" />
        </property>      
        <property name="updateTime" type="string">
            <column name="updatetime" />
        </property>
         </class>
</hibernate-mapping>