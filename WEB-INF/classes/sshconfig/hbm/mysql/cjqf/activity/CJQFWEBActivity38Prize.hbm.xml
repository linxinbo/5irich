<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wirich.bean.cjqfweb.activity.CJQFWEBActivity38Prize" table="cjqfweb_activity_38_prize">
		<!-- 主键Id -->
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="native"></generator>
		</id>
		<!-- 奖品名称 -->
		<property name="prizeName" type="java.lang.String">
			<column name="prize_name" />
		</property>
		<!-- 奖品原价 -->
		<property name="prizeOriginal" type="java.lang.String">
			<column name="prize_original" />
		</property>
		<!-- 奖品最低价 -->
		<property name="prizeDiscount" type="java.lang.String">
			<column name="prize_discount" />
		</property>
	</class>
</hibernate-mapping>
