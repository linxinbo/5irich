<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wirich.bean.wind.mutual.ChinaMutualFundBondPortfolioBean" table="chinamutualfundbondportfolio">
		<!-- 主键Id -->
		<id name="ObjectId" type="java.lang.String">
			<column name="OBJECT_ID" />
		</id>
		<!--  Wind 代码 -->
		<property name="sinfoWindcode" type="java.lang.String">
			<column name="S_INFO_WINDCODE" />
		</property>
		<!--  截止日期  -->
		<property name="fprtEndDate" type="java.lang.String">
			<column name="F_PRT_ENDDATE" />
		</property>
		<!--货币代码  -->
		<property name="crncyCode" type="java.lang.String">
			<column name="CRNCY_CODE" />
		</property>
		<!--持有债券 Wind 代码  -->
		<property name="sinfoBondWindCode" type="java.lang.String">
			<column name="S_INFO_BONDWINDCODE" />
		</property>
		<!--持有债券市值(元)  -->
		<property name="fprtBdValue" type="java.math.BigDecimal">
			<column name="F_PRT_BDVALUE" />
		</property>
		<!--持有债券数量（张）  -->
		<property name="fprtBdQuantity" type="java.math.BigDecimal">
			<column name="F_PRT_BDQUANTITY" />
		</property>
		<!--持有债券市值占基金净值比例(%)  -->
		<property name="fprtBdValueTonav" type="java.math.BigDecimal">
			<column name="F_PRT_BDVALUETONAV" />
		</property>
		<!--  -->
		<property name="OpDate" type="java.util.Date">
			<column name="OPDATE" />
		</property>
		<!--  -->
		<property name="OpMode" type="java.lang.String">
			<column name="OPMODE" />
		</property>
			</class>
</hibernate-mapping>
