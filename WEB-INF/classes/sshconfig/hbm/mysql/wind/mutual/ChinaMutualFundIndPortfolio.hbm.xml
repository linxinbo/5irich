<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wirich.bean.wind.mutual.ChinaMutualFundIndPortfolioBean" table="chinamutualfundindportfolio">
		<!-- 主键Id -->
		<id name="ObjectId" type="java.lang.String">
			<column name="OBJECT_ID" />
		</id>
		<!-- Wind 代码 -->
		<property name="sInfoWindcode" type="java.lang.String">
			<column name="S_INFO_WINDCODE" />
		</property>
		<!-- 截止日期 -->
		<property name="fPrtEnddate" type="java.lang.String">
			<column name="F_PRT_ENDDATE" />
		</property>
		<!--证监会行业编号  -->
		<property name="sInfoCsrcinduscode" type="java.lang.String">
			<column name="S_INFO_CSRCINDUSCODE" />
		</property>
		<!-- 持有行业市值(元) -->
		<property name="fPrtIndusvalue" type="java.math.BigDecimal">
			<column name="F_PRT_INDUSVALUE" />
		</property>
		<!-- 持有行业市值占基金净值比例(%) -->
		<property name="fPrtIndustonav" type="java.math.BigDecimal">
			<column name="F_PRT_INDUSTONAV" />
		</property>
		<!-- 积极投资持有行业市值(元) -->
		<property name="fPrtIndposvalue" type="java.math.BigDecimal">
			<column name="F_PRT_INDPOSVALUE" />
		</property>
		<!-- 积极投资持有行业比例(%) -->
		<property name="fPrtIndpospro" type="java.math.BigDecimal">
			<column name="F_PRT_INDPOSPRO" />
		</property>
		<!-- 指数投资持有行业市值(元) -->
		<property name="fPrtIndpassivevalue" type="java.math.BigDecimal">
			<column name="F_PRT_INDPASSIVEVALUE" />
		</property>
		<!-- 指数投资持有行业比例(%) -->
		<property name="fPrtIndpassivepro" type="java.math.BigDecimal">
			<column name="F_PRT_INDPASSIVEPRO" />
		</property>
			</class>
</hibernate-mapping>
