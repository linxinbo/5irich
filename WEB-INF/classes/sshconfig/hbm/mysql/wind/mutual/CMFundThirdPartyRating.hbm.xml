<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wirich.bean.wind.mutual.CMFundThirdPartyRatingBean" table="cmfundthirdpartyrating">
		<!-- 主键Id -->
		<id name="ObjectId" type="java.lang.String">
			<column name="OBJECT_ID" />
		</id>
		<!-- Wind代码  -->
		<property name="sInfoWindcode" type="java.lang.String">
			<column name="S_INFO_WINDCODE" />
		</property>
		<!-- 日期（月份）  -->
		<property name="rptdate" type="java.lang.String">
			<column name="RPTDATE" />
		</property>
		<!-- 星级;  -->
		<property name="starlevel" type="java.math.BigDecimal">
			<column name="STARLEVEL" />
		</property>
		<!-- 基金类型;  -->
		<property name="fInfoType" type="java.lang.String">
			<column name="F_INFO_TYPE" />
		</property>
		<!-- 评级区间  -->
		<property name="ratingInterval" type="java.lang.String">
			<column name="RATING_INTERVAL" />
		</property>
		<!-- 评级机构  -->
		<property name="ratingGagency" type="java.lang.String">
			<column name="RATING_GAGENCY" />
		</property>
		<!-- 未知  -->
		<property name="opdate" type="java.util.Date">
			<column name="OPDATE" />
		</property>
		<!-- 未知  -->
		<property name="opmode" type="java.lang.String">
			<column name="OPMODE" />
		</property>
		
	</class>
</hibernate-mapping>
