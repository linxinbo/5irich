<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wirich.bean.wind.mutual.CmfRiskLevelBean" table="cmfrisklevel">
		<!-- 主键Id -->
		<id name="ObjectId" type="java.lang.String">
			<column name="OBJECT_ID" />
		</id>
		<!-- Wind代码  -->
		<property name="f_info_windcode" type="java.lang.String">
			<column name="f_info_windcode" />
		</property>
		<!-- 基金风险等级 -->
		<property name="risk_level" type="java.lang.String">
			<column name="risk_level" />
		</property>
		<!-- 日期（月份）  -->
		<property name="opdate" type="java.lang.String">
			<column name="opdate" />
		</property>
		<!-- 基金类型 -->
		<property name="opmode" type="java.lang.String">
			<column name="opmode" />
		</property>
	</class>
</hibernate-mapping>
