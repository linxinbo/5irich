   <!-- 输出公募基金列表json -->
	<bean id="MutualFundListManacheAction" class="com.wirich.action.manache.mutual.MutualFundListManacheAction" scope="prototype">
	<property name="manacheService" ref="MutualFundListManacheServiceImpl"></property>
	</bean>
