	<bean id="MutualFundListManacheQueryAction" class="com.wirich.action.manache.mutual.MutualFundListManacheQueryAction" scope="prototype">
	<property name="manacheService" ref="MutualFundListManacheQueryServiceImpl"></property>
	</bean>
