	<bean id="CjqfwebIFMUserdaoImpl" class="com.wirich.dao.impl.intelligentfinancialmanagement.CjqfwebIFMUserdaoImpl" scope="prototype">
	</bean>
	<bean id="CjqfwebIFMQuestionsdaoImpl" class="com.wirich.dao.impl.intelligentfinancialmanagement.CjqfwebIFMQuestionsdaoImpl" scope="prototype">
	</bean>
	<bean id="CJQFWebGroupFundMainDaoImpl2" class="com.wirich.dao.cjqf.group.impl.CJQFWebGroupFundMainDaoImpl">
	</bean>
	<bean id="CJQFWebFundGroupFundDaoImpl2" class="com.wirich.dao.cjqf.group.impl.CJQFWebFundGroupFundDaoImpl">
	</bean>
	<!--智能理财方案-->
	<bean id="IFMSchemeAction" class="com.wirich.action.intelligentfinancialmanagement.IFMSchemeAction" scope="prototype">
		<property name="schemeService" ref="IFMSchemeServicImpl"></property>
	</bean> 
	<bean id="IFMSchemeServicImpl" class="com.wirich.service.impl.intelligentfinancialmanagement.IFMSchemeServicImpl" scope="prototype">
