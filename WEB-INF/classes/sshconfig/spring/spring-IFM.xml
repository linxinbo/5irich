       http://www.springframework.org/schema/mvc/spring-mvc-3.1.xsd" >	
    
    <!--智能理财问题-->
	<bean id="IFMQuestionQueryAction" class="com.wirich.action.intelligentfinancialmanagement.IFMQuestionQueryAction" scope="prototype">
		<property name="queryService" ref="IFMQuestionQueryServiceImpl"></property>
	</bean> 
	<bean id="IFMQuestionQueryServiceImpl" class="com.wirich.service.impl.intelligentfinancialmanagement.IFMQuestionQueryServiceImpl" scope="prototype">
		<property name="questionsDao" ref="CjqfwebIFMQuestionsdaoImpl"></property>
	</bean>
	<bean id="IFMQuestionQueryAcheServiceImpl" class="com.wirich.service.impl.intelligentfinancialmanagement.IFMQuestionQueryAcheServiceImpl" scope="prototype">
		<property name="questionsDao" ref="CjqfwebIFMQuestionsdaoImpl"></property>
		<property name="cjqfWebGroupFundMainDao" ref="CJQFWebGroupFundMainDaoImpl2"></property>
		<property name="cjqfWebFundGroupFundDao" ref="CJQFWebFundGroupFundDaoImpl2"></property>
	</bean>
	
	<!--智能理财问题评分-->
	<bean id="IFMScoreAction" class="com.wirich.action.intelligentfinancialmanagement.IFMScoreAction" scope="prototype">
		<property name="scoreService" ref="IFMScoreServiceImpl"></property>
	</bean> 
	<bean id="IFMScoreServiceImpl" class="com.wirich.service.impl.intelligentfinancialmanagement.IFMScoreServiceImpl" scope="prototype">
		<property name="scoreDao" ref="CjqfwebIFMScoredaoImpl"></property>
		<property name="userdao" ref="CjqfwebIFMUserdaoImpl"></property>
	</bean>
	<bean id="CjqfwebIFMScoredaoImpl" class="com.wirich.dao.impl.intelligentfinancialmanagement.CjqfwebIFMScoredaoImpl" scope="prototype">
